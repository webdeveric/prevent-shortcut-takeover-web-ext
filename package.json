{
  "name": "@webdeveric/prevent-shortcut-takeover-web-ext",
  "private": true,
  "version": "0.2.5",
  "license": "MIT",
  "description": "Stop sites from taking over your keyboard shortcuts",
  "keywords": [
    "WebExtension",
    "web-extension",
    "prevent",
    "shortcut",
    "takeover"
  ],
  "author": {
    "email": "eric@webdeveric.com",
    "name": "Eric King",
    "url": "http://webdeveric.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdeveric/prevent-shortcut-takeover-web-ext.git"
  },
  "bugs": {
    "url": "https://github.com/webdeveric/prevent-shortcut-takeover-web-ext/issues"
  },
  "homepage": "https://github.com/webdeveric/prevent-shortcut-takeover-web-ext",
  "scripts": {
    "lint": "eslint ./*.js ./src --ext .ts,.tsx,.js --fix",
    "spellcheck": "cspell --no-progress \"./{.github,src}/**/*.{js,json}\" \"./*.{md,js}\" \"./package.json\"",
    "format": "prettier --write .",
    "web-ext": "web-ext build",
    "postweb-ext": "web-ext lint",
    "prebuild": "npm run spellcheck",
    "build": "NODE_ENV=production webpack",
    "start": "NODE_ENV=development webpack --watch",
    "presign": "npm run build",
    "sign": "web-ext sign",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "version": "sync-package-version ./src/manifest.json | xargs git add",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ],
    "*.{js,jsx,ts,tsx,json,md,css}": [
      "cspell lint --no-progress --no-summary --no-must-find-files",
      "prettier --write"
    ]
  },
  "prettier": "@webdeveric/prettier-config",
  "browserslist": [
    "extends @webdeveric/browserslist-config/web-extensions"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/webextension-polyfill": "^0.9.0",
    "@webdeveric/browserslist-config": "^0.2.0",
    "@webdeveric/eslint-config-ts": "^0.5.2",
    "@webdeveric/prettier-config": "^0.2.0",
    "@webdeveric/sync-package-version": "^0.3.0",
    "autoprefixer": "^10.2.5",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.8.3",
    "cspell": "^6.1.1",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "git-rev-sync": "^3.0.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^13.0.0",
    "mini-css-extract-plugin": "^2.0.0",
    "postcss": "^8.3.0",
    "postcss-loader": "^7.0.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ts-loader": "^9.2.1",
    "typescript": "^4.7.3",
    "web-ext": "^6.2.0",
    "web-ext-plugin": "^1.2.0",
    "webextension-polyfill": "^0.9.0",
    "webpack": "^5.38.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.7.0"
  }
}
