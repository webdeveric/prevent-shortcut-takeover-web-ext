{
  "include": ["src/**/*", "typings"],
  "compilerOptions": {
    "jsx": "react",
    "resolveJsonModule": true,
    "target": "ES2020",
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "outDir": "./dist",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true
  }
}
